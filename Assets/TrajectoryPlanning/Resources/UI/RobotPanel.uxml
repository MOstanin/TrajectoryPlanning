<ui:UXML xmlns:ui="UnityEngine.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/TrajectoryPlanning/Resources/UI/RobotPanel.uss?fileID=7433441132597879392&amp;guid=5c5726d9af5a51c49934c75f108a67b1&amp;type=3#RobotPanel" />
    <ui:VisualElement name="Root" class="root">
        <ui:VisualElement class="panel" style="width: 30%;">
            <ui:Label name="ModelIdLabel" text="Model: -" class="title" />
            <ui:Label name="DofLabel" text="DOF: -" class="subtitle" />
            <ui:VisualElement name="TopTabs" class="toolbar">
                <ui:Toggle name="StateTab" text="State" value="true"/>
                <ui:Toggle name="TargetsTab" text="Targets"/>
                <ui:Toggle name="VelocityTab" text="Velocity"/>
                <ui:Toggle name="AccelerationTab" text="Acceleration"/>
            </ui:VisualElement>

            <ui:VisualElement name="StateSection">
                <ui:ScrollView name="JointsScroll" class="scroll"/>
            </ui:VisualElement>

            <ui:VisualElement name="TargetsSection" style="display: none;">
                <ui:ScrollView name="TargetsScroll" class="scroll"/>
                <ui:Button name="MoveButton" text="Move" style="bottom: 0;"/>
                <ui:Button name="StopButton" text="Stop" style="bottom: 0;"/>
            </ui:VisualElement>
            <ui:VisualElement name="VelocitySection" style="display: none;">
                <ui:ScrollView name="VelocityLimitsScroll" class="scroll"/>
            </ui:VisualElement>
            <ui:VisualElement name="AccelerationSection" style="display: none;">
                <ui:ScrollView name="AccelerationLimitsScroll" class="scroll"/>
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
